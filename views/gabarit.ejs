<!DOCTYPE html>
<html lang="fr">
<% include composants/head %>
<body>
 <table class="tableau">
    <% let noLigne = 0%>
    <% for(elm of adresses) {%>
          <%  if (noLigne==0){%>
                <tr>
                     <% for(info in elm) {%>
                        <td>
                            <% noLigne++%>
                            <%= info %>
                        </td>
                     <%}%>
                </tr>
            <%}%>

                <tr>
                    <% for(info in elm) {%>
                         <td><%= elm[info] %></td>
                    <%}%>
                 </tr>
    <% } %>


 </table>
</body>
</html>